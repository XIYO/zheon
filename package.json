{
	"name": "zheon",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:wrangler": "wrangler dev --port 5170",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:integration": "vitest run tests/integration",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"deploy": "pnpm build && pnpm exec wrangler deploy",
		"wrangler": "wrangler",
		"edge:test:all": "cd supabase && deno task test",
		"edge:deploy": "supabase functions deploy --project-ref iefgdhwmgljjacafqomd",
		"edge:deploy:with-secrets": "supabase secrets set --env-file=.env --project-ref iefgdhwmgljjacafqomd && supabase functions deploy --project-ref iefgdhwmgljjacafqomd",
		"edge:secrets:set": "supabase secrets set --env-file=.env --project-ref iefgdhwmgljjacafqomd",
		"edge:secrets:list": "supabase secrets list --project-ref iefgdhwmgljjacafqomd",
		"edge:format": "deno fmt supabase/functions/",
		"edge:lint": "deno lint supabase/functions/",
		"edge:check": "cd supabase/functions && for dir in */; do [ -f \"$dir/index.ts\" ] && deno check \"$dir/index.ts\"; done",
		"edge:debug": "supabase functions serve --inspect-mode --project-ref iefgdhwmgljjacafqomd",
		"edge:debug:brk": "supabase functions serve --inspect-mode brk --project-ref iefgdhwmgljjacafqomd"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.56.1",
		"@skeletonlabs/skeleton": "^4.2.4",
		"@skeletonlabs/skeleton-svelte": "^4.2.4",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"@vitest/browser": "^4.0.7",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"globals": "^16.5.0",
		"jsdom": "^27.1.0",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.1",
		"vitest": "^4.0.7",
		"vitest-browser-svelte": "^2.0.1",
		"wrangler": "^4.45.4"
