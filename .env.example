# Supabase
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-anon-key
SUPABASE_SECRET_KEY=your-service-role-key

# Database
DATABASE_URL=postgresql://postgres:password@host:port/database

# AI Models
GEMINI_API_KEY=your-gemini-api-key  # Gemini 2.5 Flash (요약) + TTS

# TTS (Text-to-Speech) - 한국어 음성 생성
# LMNT TTS (한국어)
LMNT_API_KEY=your-lmnt-api-key
LMNT_TTS_MODEL=blizzard  # 기본 모델 (aurora는 blizzard alias)
LMNT_TTS_VOICE=natalie  # 보이스 ID (소문자)
LMNT_TTS_LANGUAGE=ko
LMNT_TTS_FORMAT=mp3
LMNT_TTS_SPEED=1.0

# ElevenLabs TTS (한국어) - 최고 품질
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_TTS_MODEL=eleven_v3  # Eleven v3 Alpha (70+ 언어, 최고 감정 표현, 알파 버전)
# ELEVENLABS_TTS_MODEL=eleven_multilingual_v2  # Multilingual v2 (프로덕션 안정, 32개 언어)
# ELEVENLABS_TTS_MODEL=eleven_turbo_v2_5  # Turbo v2.5 (품질과 속도 균형)
ELEVENLABS_TTS_VOICE=B8gJV1IhpuegLxdpXFOE  # 한국어 보이스 ID (플레이그라운드에서 복사)
ELEVENLABS_TTS_FORMAT=mp3_44100_128
ELEVENLABS_TTS_SPEED=1.0

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# YouTube Data API v3
YOUTUBE_API_KEY=your-youtube-api-key

# Proxy Settings
SOCKS5_PROXY_HOST=proxy-host
SOCKS5_PROXY_PORT=proxy-port
HEADER_PROXY_URL=proxy-url